{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "class-methods-use-this": [0],
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off"
  },
  "env": {
    "commonjs": true,
    "node": true,
    "jest": true
  }
}
